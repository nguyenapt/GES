@using GES.Common.Resources
@using GES.Inside.Data.Models.CaseProfiles
@model ICaseProfileEngagementInformationViewModel

@{
    var engagementInformationPartial = Html.Partial("CaseProfiles/Shared/EngagementInformation", Model as CaseProfileEngagementInformationViewModel).ToString();
}

@if (!string.IsNullOrWhiteSpace(engagementInformationPartial))
{
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">
                @Resources.EngagementInformation
            </h3>
        </div><!-- /.box-header -->
        <div class="box-body">
            @Html.Raw(engagementInformationPartial)
        </div>
    </div>
    
}