@using GES.Common.Resources
<script type="text/javascript" charset="utf-8">
    var _caseReportId = @ViewBag.CaseReportId;
</script>

<div class="box box-primary" id="div-additional-incident">
    <div class="box-header with-border">
        <h3 class="box-title">
            <i class="fa fa-list-alt" aria-hidden="true"></i> @Resources.CompanyRelatedItems
        </h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div id="companyRelatedItemsTableContainer">
            <table id="tblcases" class="table table-bordered table-striped"></table>
            <div id="tblcases-pager"></div>
        </div>
        <div class="no-company-related-items-msg no-result-found-message">@Resources.NoCompanyRelatedCaseProfilesFound</div>
    </div>
</div>

@{ Html.RenderPartial("_UnSubscribed_Box_Message"); }
