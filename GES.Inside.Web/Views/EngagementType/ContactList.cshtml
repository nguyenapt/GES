@using GES.Common.Resources
@model GES.Inside.Data.Models.EngagementTypeViewModel
@{

}
<div class="modal fade" id="engagement-type-contacts-dialog" style="display: none;">
    <div class="modal-dialog modal-lg" style="width: 800px; height: auto">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title text-bold">@Resources.EngagementTypeSelectContactInformation</h4>
            </div>
            <div class="modal-body" style="min-height: 0; max-height: 600px; overflow-y: auto">
                <div class="">
                    <table id="tblcontact" class="table table-bordered table-striped"></table>
                    <div id="myPager"></div>
                </div>
            </div>
            <div class="modal-footer">
                 <input class="btn btn-success" type="button" value="Select" data-dismiss="modal" id="btn-contact-select" ng-click="contactSelect()"  />
                <input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel" />
            </div>
        </div>
    </div>
</div>