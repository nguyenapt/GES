@using GES.Inside.Web.Helpers
@{
    ViewBag.Title = "Engagement types";
    ViewBag.PageClass = "page-engagement-types page-engagement-types-index";
}

@section scripts {
    @Scripts.Render("~/bundles/page-engagement-types-list")
}

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                @*<h3 class="box-title">Data Table With Full Features</h3>*@
            </div><!-- /.box-header -->
            <div class="box-body">
                 <input class="btn btn-success" type="button" value="Add New Engagement Type" onclick="window.location = '/EngagementType/EngagementTypeDetails/Add'" />


                <table id="tblEngagementTypes" class="table table-bordered table-striped"></table>
                <div id="myPager"></div>
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div><!-- /.col -->
</div><!-- /.row -->