@using GES.Inside.Web.Helpers
@{
    ViewBag.PageClass = "page-signup page-signup-case-profile-list";
    ViewBag.Title = string.Format("Signed up case profiles");
}
@section scripts {
    @Scripts.Render("~/bundles/page-case-report-signup-list")
}

<div class="row">
    <div class="col-xs-12">
        <div class="box">

            <div class="box-body">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 text-right">
                        <input class="btn export-endorsements-btn btn-primary not-outer-right-btn pull-right" type="button" data-loading-text="Processing..." value="Export Endorsements to Excel" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12">
                        <table id="tblcasereportsignuplist" class="table table-bordered table-striped"></table>
                        <div id="myPager"></div>
                    </div>
                </div>
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div><!-- /.col -->
</div><!-- /.row -->
